{
  "name": "rogelio1502/ef-orchid-package",
  "description": "EF Orchid Package",
  "version": "1.0.52",
  "type": "library",
  "require": {
    "php": ">=8.0",
    "orchid/platform": "^13.9"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Rogelio Torres Pasillas",
      "email": "rogelio15021515@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "EF\\Components\\": "src/EF/Components/",
      "EF\\Providers\\": "src/EF/Providers/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EF\\Providers\\EfServiceProvider"
      ]
    }
  },
  "post-update-cmd": [
    "php artisan clear-compiled",
    "php artisan optimize",
    "npx mix"
  ],
  "post-install-cmd": [
    "php artisan clear-compiled",
    "php artisan optimize",
    "npx mix"
  ]
}
